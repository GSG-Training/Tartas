<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tartas">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_cake"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_cake"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tartas">
        <activity android:name=".view.sign.SigninActivity" />
        <activity android:name=".view.sign.SignupActivity" />
        <activity android:name=".view.welcome.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.welcome.WelcomeActivity">
        </activity>
        <activity android:name=".view.MainActivity.MainActivity">
        </activity>
    </application>

</manifest>